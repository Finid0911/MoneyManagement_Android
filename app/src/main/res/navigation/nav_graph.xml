<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.moneymanagement.Views.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_account_transactionFragment"
            app:destination="@id/account_transactionFragment" />
    </fragment>
    <fragment
        android:id="@+id/accountsFragment"
        android:name="com.example.moneymanagement.Views.AccountsFragment"
        android:label="fragment_accounts"
        tools:layout="@layout/fragment_accounts" >
        <action
            android:id="@+id/action_accountsFragment_to_addUserFragment2"
            app:destination="@id/addUserFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.example.moneymanagement.Views.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/userFragment"
        android:name="com.example.moneymanagement.Views.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.moneymanagement.Views.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/addUserFragment"
        android:name="com.example.moneymanagement.Views.AddUserFragment"
        android:label="fragment_add_user"
        tools:layout="@layout/fragment_add_user" >
        <action
            android:id="@+id/action_addUserFragment_to_accountsFragment2"
            app:destination="@id/accountsFragment"
            app:popUpTo="@id/accountsFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/account_transactionFragment"
        android:name="com.example.moneymanagement.Views.Account_transactionFragment"
        android:label="fragment_account_transaction"
        tools:layout="@layout/fragment_account_transaction" >
        <action
            android:id="@+id/action_account_transactionFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
    </fragment>
</navigation>